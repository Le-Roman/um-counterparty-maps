{
  "name": "um-counterparty-maps",
  "version": "1.0.0",
  "description": "Service for managing counterparty maps with competitors",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/server.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon src/server.ts",
    "start:prod": "cross-env NODE_ENV=production nodemon src/server.ts",
    "start:auto:prod": "pm2 start ecosystem.config.js --watch --only um-counterparty-maps",
    "db:migrate:dev": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "db:migrate:undo:all:dev": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo:all",
    "db:migrate:prod": "cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "db:migrate:undo:all:prod": "cross-env NODE_ENV=production npx sequelize-cli db:migrate:undo:all"
  },
  "dependencies": {
    "axios": "1.13.2",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.18.2",
    "moment": "^2.30.1",
    "mysql2": "3.6.0",
    "pm2": "5.3.0",
    "sequelize": "6.37.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.17",
    "@types/node": "20.4.5",
    "cross-env": "7.0.3",
    "nodemon": "3.0.1",
    "sequelize-cli": "6.6.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "author": "Roman L."
}
